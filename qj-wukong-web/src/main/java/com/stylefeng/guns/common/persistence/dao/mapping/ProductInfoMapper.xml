<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.common.persistence.dao.ProductInfoMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.stylefeng.guns.common.persistence.model.ProductInfo">
		<id column="id" property="id" />
		<result column="product_id" property="productId" />
		<result column="stock_num" property="stockNum" />
		<result column="price" property="price" />
		<result column="colour" property="colour" />
		<result column="dimens" property="dimens" />
		<result column="created_by" property="createdBy" />
		<result column="created_date" property="createdDate" />
		<result column="last_updated_by" property="lastUpdatedBy" />
		<result column="last_update_date" property="lastUpdateDate" />
		<result column="data_status" property="dataStatus" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, product_id AS productId, stock_num AS stockNum, price, colour, dimens, created_by AS createdBy, created_date AS createdDate, last_updated_by AS lastUpdatedBy, last_update_date AS lastUpdateDate, data_status AS dataStatus
    </sql>

</mapper>
